{
  "output": "backup-tool.phar",
  "chmod": "0755",
  "compactors": [
    "Herrera\\Box\\Compactor\\Php"
  ],
  "compression": "GZ",
  "main":"init.php",
  "directories": [
    "app",
    "src"
  ],
  "files": [
    "init.php",
    "vendor/autoload.php"
  ],
 "finder": [
    {
      "name": "/(\\.php|\\.json|\\.yml|\\.twig)$/",
      "exclude": ["Tests","tests", "docs"],
      "in": "vendor"
    }
  ]
}